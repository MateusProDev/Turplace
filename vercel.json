{
  "version": 2,
  "builds": [
    { "src": "api/[...slug].cjs", "use": "@vercel/node" },
    { "src": "api/status.cjs", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/api/status", "dest": "/api/status.cjs" },
    { "src": "/api/(.*)", "dest": "/api/[...slug].cjs" }
  ]
}
