{
  "version": 2,
  "builds": [
    { "src": "api/[...slug].js", "use": "@vercel/node" },
    { "src": "api/status.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/api/(.*)", "dest": "/api/$1.cjs" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
